{
  "name": "utms-reader",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "ingest": "ts-node -r esm ./src/ingest/cli.ts --source $UTMS_MANUSCRIPT_EXPORT --input ./manuscript --output ./manuscript.json",
