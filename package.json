    "update": "npm run build-static && git add . && git commit -am \"Build: $(date)\""
  },
  "dependencies": {
    "@types/node": "20.2.5",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "classnames": "^2.3.2",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "esm": "^3.2.25",
    "katex": "^0.16.8",
    "next": "13.4.4",
    "node-html-parser": "^6.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.9.0",
    "ts-node": "^10.9.1",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "@types/katex": "^0.16.1",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "tsconfig-paths": "^4.2.0",
    "yargs": "^17.7.2"
  }
